#include <iostream>
#include <vector>
#include <queue>
#include <string>
using namespace std;

vector<string> findRelativeRanks(vector<int>& score) {
    int n = score.size();

    priority_queue<pair<int, int>> pq;


    for (int i = 0; i < n; i++) {
        pq.push({score[i], i});
    }

    vector<string> answer(n);
    int rank = 1;

    while (!pq.empty()) {
        int idx = pq.top().second;
        pq.pop();

        if (rank == 1)
            answer[idx] = "Gold Medal";
        else if (rank == 2)
            answer[idx] = "Silver Medal";
        else if (rank == 3)
            answer[idx] = "Bronze Medal";
        else
            answer[idx] = to_string(rank);

        rank++;
    }

    return answer;
}

int main() {
    vector<int> score = {10, 3, 8, 9, 4};

    vector<string> result = findRelativeRanks(score);

    cout << "Ranks: ";
    for (string s : result)
        cout << s << " ";
    cout << endl;


    return 0;
}
